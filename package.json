{
  "name": "ws-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run start:online",
    "start:online": "node ./scripts/beforeStart.js && egg-scripts start --daemon --workers=1 --title=ws-project",
    "stop": "egg-scripts stop --title=ws-project",
    "dev": "node ./scripts/beforeStart.js -e ENV_QA && EGG_SERVER_ENV=dev egg-bin dev",
    "lint": "next lint",
    "prod": "next build && next start",
    "analyze": "ANALYZE=true next build && next bundle-analyzer",
    "build": "npm run next:build && rm -rf app/view/.next/cache",
    "next:build": "next build app/view",
    "next:dev": "next app/view",
    "next:online": "next:build && pm2 start ecosystem.config.js"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "-": "^0.0.1",
    "@types/node": "20.2.3",
    "@types/react": "18.2.7",
    "@types/react-dom": "18.2.4",
    "axios": "^1.4.0",
    "compression": "^1.7.4",
    "D": "^1.0.0",
    "egg": "^2.29.1",
    "egg-cors": "^2.2.3",
    "egg-logrotator-forward": "^3.1.0",
    "egg-mongoose": "^3.3.1",
    "egg-oss": "^2.0.0",
    "egg-passport": "^2.1.1",
    "egg-scripts": "^2.13.0",
    "egg-view-nunjucks": "^2.3.0",
    "eslint": "8.40.0",
    "eslint-config-next": "13.2.4",
    "fs-extra": "^10.0.0",
    "leancloud-storage": "^4.15.0",
    "less": "^4.1.3",
    "less-loader": "^11.1.3",
    "moment": "^2.29.4",
    "mongoose-auto-increment": "^5.0.1",
    "next": "13.4.3",
    "next-transpile-modules": "^10.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.63.5",
    "typescript": "5.0.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "antd": "^5.5.2",
    "babel-plugin-styled-components": "^2.1.3",
    "concurrently": "^8.2.0",
    "egg-bin": "^4.20.0",
    "next-bundle-analyzer": "^0.6.7",
    "next-with-less": "^3.0.1",
    "next-with-lesss": "^2.0.5"
  }
}
